Tests with namespaces.
---
<?php

namespace Test;

function test() {
     \is_int(123);
     is_int(123);
}
---
<?php

namespace Test;

function test()
{
    \burning_capture_function_return('{{__FILE__}}', 0, true, \is_int(123));
    \burning_capture_function_return('{{__FILE__}}', 1, !function_exists('Test\\is_int'), is_int(123));
}
