Tests with arrays.
---
<?php

$arr[is_int(123)];
$arr[is_int(123)] = 123;
$arr[] = [is_int(123) => is_int(123)];

list(is_int(123)) = is_int(123);

yield is_int(123);
$x = (yield is_int(123) => is_int(123));
$x = yield is_int(123);
yield from is_int(123);
