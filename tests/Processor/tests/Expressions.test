Tests with general expressions.
---
<?php

(int) is_int(123);

clone is_int(123);

empty($arr[is_int(123)]);

@is_int(123);

return is_int(123);

eval(is_int(123));

exit(is_int(123));

include is_int(123);

isset($arr[is_int(123)], $arr[is_int(123)]);

print is_int(123);

unset($arr[is_int(123)], $arr[is_int(123)]);
